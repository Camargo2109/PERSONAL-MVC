            <a href="#home">Início</a>
            <a href="#about">Sobre</a>
            <a href="#features">Exercícios</a>
            <a href="#trainers">Redes Sociais</a>
        </nav>

    </header>


    <main>
        @RenderBody()
    </main>

    <footer class="footer">
        <div class="box-container">
            <div class="box">
                <h3>Links rápidos</h3>
                <a class="links" href="#home">Início</a>
                <a class="links" href="#about">Sobre</a>
                <a class="links" href="#exercises">Exercícios</a>
                <a class="links" href="#trainers">Treinadores</a>
            </div>

            <div class="box">
